{
  "version": 2,
  "projects": {
    "common-request": {
      "root": "libs/common/request",
      "sourceRoot": "libs/common/request/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/common/request/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/common/request"],
          "options": {
            "jestConfig": "libs/common/request/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "jira": {
      "root": "apps/jira",
      "sourceRoot": "apps/jira/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/jira",
            "index": "apps/jira/src/index.html",
            "main": "apps/jira/src/main.tsx",
            "polyfills": "apps/jira/src/polyfills.ts",
            "tsConfig": "apps/jira/tsconfig.app.json",
            "assets": ["apps/jira/src/favicon.ico", "apps/jira/src/assets"],
            "styles": ["apps/jira/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/jira/src/environments/environment.ts",
                  "with": "apps/jira/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "jira:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "jira:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/jira/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/jira"],
          "options": {
            "jestConfig": "apps/jira/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "jira-e2e": {
      "root": "apps/jira-e2e",
      "sourceRoot": "apps/jira-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/jira-e2e/cypress.json",
            "devServerTarget": "jira:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "jira:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/jira-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "sarair": {
      "root": "apps/sarair",
      "sourceRoot": "apps/sarair/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/sarair",
            "index": "apps/sarair/src/index.html",
            "main": "apps/sarair/src/main.tsx",
            "polyfills": "apps/sarair/src/polyfills.ts",
            "tsConfig": "apps/sarair/tsconfig.app.json",
            "assets": ["apps/sarair/src/favicon.ico", "apps/sarair/src/assets"],
            "styles": ["apps/sarair/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/sarair/src/environments/environment.ts",
                  "with": "apps/sarair/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "sarair:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "sarair:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/sarair/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/sarair"],
          "options": {
            "jestConfig": "apps/sarair/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "sarair-e2e": {
      "root": "apps/sarair-e2e",
      "sourceRoot": "apps/sarair-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/sarair-e2e/cypress.json",
            "devServerTarget": "sarair:serve",
            "tsConfig": "apps/sarair-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "sarair:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/sarair-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "shared-hooks": {
      "root": "libs/shared/hooks",
      "sourceRoot": "libs/shared/hooks/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/shared/hooks/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/hooks"],
          "options": {
            "jestConfig": "libs/shared/hooks/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-utils": {
      "root": "libs/shared/utils",
      "sourceRoot": "libs/shared/utils/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/shared/utils/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/utils"],
          "options": {
            "jestConfig": "libs/shared/utils/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "sarair"
}
