{
  "npmScope": "sarair",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "common-request": {
      "tags": []
    },
    "jira": {
      "tags": []
    },
    "jira-e2e": {
      "tags": [],
      "implicitDependencies": ["jira"]
    },
    "sarair": {
      "tags": []
    },
    "sarair-e2e": {
      "tags": [],
      "implicitDependencies": ["sarair"]
    },
    "shared-hooks": {
      "tags": []
    },
    "shared-utils": {
      "tags": []
    }
  }
}
